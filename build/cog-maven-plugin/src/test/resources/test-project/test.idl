    scope inner {
        //option byteorder native;
        
        struct FlatParent
        {
            uint64 fixed1;
        }

        struct Flat extends FlatParent [0x10000001]
        {
            uint16 fixed2 = 222;
            string8 string1;
            uint24 fixed3 = 333;
            string8 string2;
            int32 fixed4 = 444;
            string8 string3;
            int8 fixed5 = 55;
            varstring string4;
        }

        struct Nested
        {
            uint64 fixed4 = 444;
            Flat flat;
            int64 fixed5;
        }

        struct FlatWithArray
        {
            uint64 fixed1 = 111;
            string8 string1;
            string8[] array1;
            int32 fixed2 = 5;
        }

        struct FlatWithOctets
        {
            uint32 fixed1 = 11;

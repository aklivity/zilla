property networkAcceptWindow 8192

accept "zilla://streams/net0"
  option zilla:window ${networkAcceptWindow}
  option zilla:transmission "duplex"
  option zilla:byteorder "network"

accepted

connected

read 62                                           # size
     12s                                           # Sync Group
     1s                                            # v1
     ${newRequestId}
     -1s                                           # no client id
     4s "test"                                     # Consumer Group
     3                                             # Generation id
     36s "134d7851-0dde-46a7-9483-a310ba44840b"    # Consumer Member Group Id

write 10                                              # size
      ${newRequestId}
      0                                               # Throttle time
      0s                                              # No error

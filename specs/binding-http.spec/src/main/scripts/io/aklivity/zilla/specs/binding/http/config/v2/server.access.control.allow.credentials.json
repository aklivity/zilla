{
    "name": "test",
    "bindings":
    {
        "net0":
        {
            "type" : "http",
            "kind": "server",
            "options":
            {
                "versions": [ "h2" ],
                "access-control":
                {
                    "allow":
                    {
                        "credentials": true
                    }
                }
            },
            "routes":
            [
                {
                    "exit": "app0",
                    "when":
                    [
                        {
                            "headers":
                            {
                                ":scheme": "https",
                                ":authority": "example.com:9090"
                            }
                        }
                    ]
                }
            ]
        }
    }
}

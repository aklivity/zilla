#SERVER
accept "http://localhost:8081/resource"
accepted
connected
read http:method "GET"
read http:header "Host" "localhost:8081"
read http:header "via" "1.1 proxy1, 1.1 localhost:8080"
read closed
write http:status "200" "OK"
write http:content-length
write http:header "via" "1.1 proxy3"
write close

